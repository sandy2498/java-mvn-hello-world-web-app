pipeline {
agent any
  environment {
   PATH = "/usr/java:$PATH"
  }
   stages { 
    stage("Clean code") { 
      steps { 
      sh "git clone https://github.com/sandy2498/java-mvn-hello-world-web-app"
      }
      }
      stage("build code"){
      steps{
         sh "sudo apt-get install default-jdk"
      }
    }
    }
    }
